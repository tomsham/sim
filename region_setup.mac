#Set the General Particle Source to be Na22
/gps/particle ion
/gps/ion 11 22

#Position region of generated gps, should equal to the active region
/gps/pos/type Volume
/gps/pos/shape Cylinder
/gps/pos/centre 0 0 0 cm
/gps/pos/radius 9.53/2 mm
/gps/pos/halfz 0.0001 mm

#Set the energy distribution of Source, Mono (single value)
/gps/ene/type Mono
/gps/ene/mono 0. keV